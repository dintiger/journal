<div id="register">
  <h1>Register Page</h1>

  <div>
    <form action="/user/register" method="POST" enctype="multipart/form-data">
      <div>
        <label for=""></label>
        <input
          type="text"
          name="name"
          placeholder="name"
          class="form-control"
        />
      </div>
      <div>
        <label for=""></label>
        <input
          type="text"
          name="phone"
          placeholder="phone"
          class="form-control"
        />
      </div>
      <div>
        <label for=""></label>
        <input
          type="email"
          name="email"
          placeholder="email"
          class="form-control"
        />
      </div>
      <div>
        <label for=""></label>
        <input
          type="file"
          name="profilePicture"
          placeholder="profile picture"
          class="form-control"
        />
      </div>

      <div>
        <label for=""></label>
        <input
          type="password"
          name="password"
          placeholder="password"
          class="form-control"
        />
      </div>

      <button type="submit">Register</button>
    </form>
  </div>
</div>
